<%# locals: (css_classes: nil, sticky: false, **html_options) %>
<%
  data_attrs = { table_part: "header" }
  data_attrs[:sticky] = "true" if sticky
  data_attrs.merge!(html_options.delete(:data) || {})
  html_options[:data] = data_attrs
  html_options[:class] = css_classes if css_classes
%>
<thead <%= tag.attributes(html_options) %>>
  <%= yield %>
</thead>
